{{ form_start(form) }}
    <div class="row">
        <div class="form-group col-md-4">
            <div class="sub-title">Domaine d'activité</div>
            {{ form_widget(form.domaineActivite, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group col-md-4">
            <div class="sub-title">Salaire</div>
            {{ form_widget(form.salaire, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group col-md-2">
            <div class="sub-title">Prime</div>
            {{ form_widget(form.prime, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group col-md-2">
            <div class="sub-title">Loyer</div>
            {{ form_widget(form.loyer, {'attr': {'class': 'form-control'}}) }}
        </div>
    </div>
    <div class="row">
        <div class="form-group col-md-4">
            <div class="sub-title">Profession</div>
            {{ form_widget(form.profession, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group col-md-4">
            <div class="sub-title">Propriétaire ?</div>
            {{ form_widget(form.proprietaire, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group col-md-4">
            <div class="sub-title">Personnel de votre Société</div>
            {{ form_widget(form.personnel, {'attr': {'class': 'form-control'}}) }}
        </div>
    </div>
    {{ form_widget(form) }}
    <button class="btn btn-primary">{{ button_label|default('Ajouter') }}</button>
{{ form_end(form) }}
